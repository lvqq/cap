    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.2",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^0.40.4"
